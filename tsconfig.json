{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "Node10",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2015",
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  // "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "include": [
    "next-auth.d.ts", // Add this line
    "pages/**/*",
    "components/**/*",
    "utils/**/*",
    "**/*.ts", "**/*.tsx"
    // Other include paths
  ],
  "exclude": ["node_modules"]
}
